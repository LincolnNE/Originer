{
  "version": 2,
  "buildCommand": "cd frontend && npm install && npm run build",
  "outputDirectory": "frontend/.next",
  "installCommand": "npm install",
  "framework": "nextjs",
  "rewrites": [
    {
      "source": "/api/v1/:path*",
      "destination": "/api/serverless/:path*"
    },
    {
      "source": "/health",
      "destination": "/api/serverless/health"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
